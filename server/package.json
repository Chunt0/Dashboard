{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"pnpm dev:api\" \"pnpm dev:worker\"",
    "dev:api": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "dev:worker": "nodemon --watch src --ext ts --exec ts-node src/queueWorker.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:worker": "node dist/queueWorker.js"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "mime-types": "^3.0.1",
    "multer": "^2.0.1",
    "redis": "^5.6.0",
    "sharp": "^0.34.2",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/mime-types": "^3.0.0",
    "@types/multer": "^1.4.13",
    "@types/node": "^22.15.17",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10",
    "typescript": "^5.8.3"
  }
}
